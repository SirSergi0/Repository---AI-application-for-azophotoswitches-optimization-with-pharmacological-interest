url = {https://www.sciencedirect.com/science/article/pii/S0893965911000486},
author = {Kexiang Xu},
keywords = {Zagreb index, Clique number, Chromatic number},
abstract = {For a (molecular) graph, the first Zagreb index M1 is equal to the sum of squares of the degrees of vertices, and the second Zagreb index M2 is equal to the sum of the products of the degrees of pairs of adjacent vertices. Let Wn,k be the set of connected n-vertex graphs with clique number k. In this work we characterize the graphs from Wn,k with extremal (maximal and minimal) Zagreb indices, and determine the values of corresponding indices.}
}

